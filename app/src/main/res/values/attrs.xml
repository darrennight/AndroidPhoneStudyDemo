<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BGASwipeItemLayout">
        <attr name="bga_sil_swipeDirection" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="bga_sil_bottomMode" format="enum">
            <enum name="pullOut" value="0" />
            <enum name="layDown" value="1" />
        </attr>
        <attr name="bga_sil_springDistance" format="dimension" />
        <attr name="bga_sil_swipeAble" format="boolean" />
    </declare-styleable>


    <declare-styleable name="MaterialLeanBack">
        <attr name="mlb_titleColor" format="color"/>
        <attr name="mlb_titleSize" format="dimension"/>
        <attr name="mlb_overlapCards" format="boolean"/>
        <attr name="mlb_animateCards" format="boolean"/>

        <attr name="mlb_cardElevationEnlarged" format="integer"/>
        <attr name="mlb_cardElevationReduced" format="integer"/>

        <attr name="mlb_lineSpacing" format="dimension"/>

        <attr name="mlb_paddingTop" format="dimension"/>
        <attr name="mlb_paddingBottom" format="dimension"/>
        <attr name="mlb_paddingLeft" format="dimension"/>
        <attr name="mlb_paddingRight" format="dimension"/>

        <attr name="mlb_background" format="reference"/>
        <attr name="mlb_backgroundOverlay" format="float"/>
        <attr name="mlb_backgroundOverlayColor" format="color"/>

    </declare-styleable>


    <declare-styleable name="gradient">

        <attr name="transition_drawable" format="enum"/>
        <attr name="transition_duration" format="integer"/>
    </declare-styleable>

    <bool name="is_rtl">false</bool>



    <declare-styleable name="JazzyViewPager">
        <attr name="style">
            <enum name="standard" value="0" />
            <enum name="tablet" value="1" />
            <enum name="cubein" value="2" />
            <enum name="cubeout" value="3" />
            <enum name="flipvertical" value="4" />
            <enum name="fliphorizontal" value="5" />
            <enum name="stack" value="6" />
            <enum name="zoomin" value="7" />
            <enum name="zoomout" value="8" />
            <enum name="rotateup" value="9" />
            <enum name="rotatedown" value="10" />
            <enum name="accordion" value="11" />
        </attr>
        <attr name="fadeEnabled" format="boolean" />
        <attr name="outlineEnabled" format="boolean" />
        <attr name="outlineColor" format="color|reference" />
    </declare-styleable>

    <!--<declare-styleable name="Reskin">
        <attr name="enable" format="boolean"/>
    </declare-styleable>-->

    <!--<attr name="enable" format="boolean"/> -->
</resources>